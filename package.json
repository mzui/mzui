{
  "name": "mzui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e": "vue-cli-service test:e2e",
    "lint": "vue-cli-service lint",
    "style": "run-s format lint",
    "format": "run-s format:**",
    "format:src": "prettier --write \"src/**/*.ts\" \"src/**/*.vue\"  \"src/**/*.js\" ",
    "add:android": "npx cap add android",
    "build:android": "cd android && gradle assembleRelease",
    "build:android:debug": "cd android && gradle assembleDebug",
    "start:android": "run-s install-android ss log",
    "install-android": "cd android && adb install -r ./app/build/outputs/apk/debug/app-debug.apk",
    "start:android1": "cd android && adb install -r ./app/build/outputs/apk/debug/app-debug.apk && adb shell am start -n com.example.app/com.example.app.MainActivity",
    "ss": "adb shell am start -n com.example.app/com.example.app.MainActivity",
    "log": "adb logcat -d"
  },
  "dependencies": {
    "@capacitor/android": "2.4.0",
    "@capacitor/cli": "^2.4.0",
    "@capacitor/core": "^2.4.0",
    "core-js": "^3.6.5",
    "register-service-worker": "^1.7.1",
    "vue": "^2.6.11",
    "vue-class-component": "^7.2.3",
    "vue-property-decorator": "^8.4.2",
    "vue-router": "^3.2.0",
    "vuex": "^3.4.0"
  },
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/mocha": "^5.2.4",
    "@vue/cli-plugin-babel": "^4.5.0",
    "@vue/cli-plugin-e2e-cypress": "^4.5.0",
    "@vue/cli-plugin-pwa": "^4.5.0",
    "@vue/cli-plugin-router": "^4.5.0",
    "@vue/cli-plugin-typescript": "^4.5.0",
    "@vue/cli-plugin-unit-mocha": "^4.5.0",
    "@vue/cli-plugin-vuex": "^4.5.0",
    "@vue/cli-service": "^4.5.0",
    "@vue/test-utils": "^1.0.3",
    "chai": "^4.1.2",
    "commander": "^6.0.0",
    "less": "^3.0.4",
    "less-loader": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~3.9.3",
    "vue-template-compiler": "^2.6.11"
  }
}
